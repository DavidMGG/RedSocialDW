<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Lista de Imágenes</title>
</head>
<body>
  <h1>Imágenes almacenadas</h1>
  <div>
    <% imageUrls.forEach(url => { %>
      <img src="<%= url %>" alt="Imagen subida" width="300">
    <% }) %>
  </div>
</body>
</html>
